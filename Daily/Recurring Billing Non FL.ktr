  <step>
    <name>Cek Polis for Recurring</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>JBSDB</connection>
    <sql>&#xd;&#xa;SELECT 1 AS bill_id,&#xd;&#xa;	pb.&#x60;policy_Id&#x60;,&#xd;&#xa;	COALESCE&#x28;bx.&#x60;recurring_seq&#x60;,pt.&#x60;recurring_seq&#x60;,0&#x29; &#x2b;1 AS rec_seq,&#xd;&#xa;	DATE_ADD&#x28;COALESCE&#x28;bx.&#x60;due_dt_pre&#x60;,pt.&#x60;due_dt_pre&#x60;,pb.&#x60;commence_dt&#x60;&#x29;, INTERVAL pb.premium_mode MONTH&#x29; AS due_dt_pre,&#xd;&#xa;	pb.&#x60;regular_premium&#x60;,&#xd;&#xa;	pb.&#x60;cashless_fee_amount&#x60;,&#xd;&#xa;	pb.&#x60;regular_premium&#x60;&#x2b;pb.&#x60;cashless_fee_amount&#x60; AS total&#xd;&#xa;FROM &#x60;policy_billing&#x60; pb&#xd;&#xa;LEFT JOIN&#x28;&#xd;&#xa;	SELECT MAX&#x28;b.&#x60;BillingID&#x60;&#x29; AS BillingID, b.&#x60;policy_id&#x60;&#xd;&#xa;	FROM &#x60;billing&#x60; b&#xd;&#xa;	LEFT JOIN &#x60;policy_billing&#x60; pb ON pb.&#x60;policy_Id&#x60;&#x3d;b.&#x60;policy_id&#x60;&#xd;&#xa;	WHERE b.&#x60;IsClosed&#x60;&#x3d;0&#xd;&#xa;	AND pb.&#x60;Policy_status&#x60; IN &#x28;&#x27;Inforce&#x27;,&#x27;Grace&#x27;&#x29;&#xd;&#xa;	AND pb.&#x60;product_id&#x60;&#x3c;&#x3e;1 &#xd;&#xa;	GROUP BY pb.&#x60;policy_Id&#x60;&#xd;&#xa;&#x29;a ON a.policy_id&#x3d;pb.&#x60;policy_Id&#x60;&#xd;&#xa;LEFT JOIN &#x60;billing&#x60; bx ON bx.&#x60;BillingID&#x60;&#x3d;a.BillingID&#xd;&#xa;LEFT JOIN &#x60;policy_last_trans&#x60; pt ON pt.policy_Id&#x3d;pb.policy_Id&#xd;&#xa;INNER JOIN &#x60;product&#x60; pd ON pd.&#x60;product_id&#x60;&#x3d;pb.&#x60;product_id&#x60;&#xd;&#xa;WHERE pb.&#x60;Policy_status&#x60; IN &#x28;&#x27;Inforce&#x27;,&#x27;Grace&#x27;&#x29;&#xd;&#xa;AND pb.&#x60;product_id&#x60;&#x3c;&#x3e;1 &#xd;&#xa;&#x23; agar billing tidak melewati no renewal&#xd;&#xa;AND DATE_ADD&#x28;bx.&#x60;due_dt_pre&#x60;,INTERVAL pb.premium_mode MONTH&#x29; &#x3c;&#x3d; DATE_ADD&#x28;pb.due_dt, INTERVAL COALESCE&#x28;NULLIF&#x28;pd.&#x60;auto_renewal&#x60;,1&#x29;,pb.&#x60;IsRenewal&#x60;,0&#x29; MONTH&#x29;&#xd;&#xa;&#x23; agar billing tidak melewati periode sekarang&#xd;&#xa;AND DATE_FORMAT&#x28;DATE_ADD&#x28;COALESCE&#x28;bx.&#x60;due_dt_pre&#x60;,pt.&#x60;due_dt_pre&#x60;,pb.&#x60;commence_dt&#x60;&#x29;, INTERVAL pb.premium_mode MONTH&#x29;,&#x27;&#x25;Y&#x25;m&#x27;&#x29;&#x3c;&#x3d;DATE_FORMAT&#x28;CURDATE&#x28;&#x29;,&#x27;&#x25;Y&#x25;m&#x27;&#x29;&#xd;&#xa;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
    <limit>0</limit>
